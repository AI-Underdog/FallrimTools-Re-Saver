{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch ReSaver",
            "request": "launch",
            "mainClass": "resaver.ReSaver",
            "projectName": "resaver",
            "vmArgs": "--module-path \"target/lib\" --add-modules javafx.controls,javafx.swing",
            "args": "",
            "console": "internalConsole"
        },
        {
            "type": "java",
            "name": "Debug ReSaver",
            "request": "launch",
            "mainClass": "resaver.ReSaver",
            "projectName": "resaver",
            "vmArgs": "--module-path \"target/lib\" --add-modules javafx.controls,javafx.swing",
            "args": "",
            "console": "internalConsole"
        },
        {
            "type": "java",
            "name": "Launch with JavaFX",
            "request": "launch",
            "mainClass": "resaver.ReSaver",
            "projectName": "resaver",
            "vmArgs": [
                "--module-path",
                "${workspaceFolder}/target/lib",
                "--add-modules",
                "javafx.controls,javafx.swing,javafx.base,javafx.graphics"
